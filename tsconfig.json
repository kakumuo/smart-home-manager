{
    "compilerOptions": {
      "module": "ES6",
      "target": "ES6",
      "moduleResolution": "Node",
      "noImplicitAny": true,
      "strictNullChecks": false,
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*", "src/*"]
      }
    },
    "include": [
      "src/**/*"
    ]
  }